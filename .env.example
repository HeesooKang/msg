# 트레이딩 모드: paper(모의투자) / real(실전투자)
TRADING_MODE=paper

# 모의투자 API 키
PAPER_API_KEY=your_paper_app_key
PAPER_API_SECRET=your_paper_app_secret
PAPER_ACCOUNT_NUMBER=12345678

# 실전투자 API 키 (실전 전환 시 설정)
# REAL_API_KEY=your_real_app_key
# REAL_API_SECRET=your_real_app_secret
# REAL_ACCOUNT_NUMBER=87654321

# 계좌상품코드 (01:종합, 03:선물옵션, 08:해외선물)
ACCOUNT_PRODUCT_CODE=01

# HTS ID
HTS_ID=your_hts_id

# 로깅
LOG_LEVEL=INFO

# 알림 (기본 OFF)
ALERTS_ENABLED=false
ALERT_CHANNEL=slack
SLACK_WEBHOOK_URL=
ALERT_MIN_INTERVAL_SECONDS=300
